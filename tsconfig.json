{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@sac/*": ["./*"],
      "@components/*": ["src/components/*"],
      "@routes/*": ["src/routes/*"],
      "@assets/*": ["src/assets/*"],
      "@screens/*": ["src/screens/*"],
      "@services/*": ["src/services/*"],
      "@libs/*": ["src/libs/*"],
      "@@types/*": ["src/@types/*"],
      "@utils/*": ["src/utils/*"],
      "@config/*": ["src/config/*"],
    }
  }
}
